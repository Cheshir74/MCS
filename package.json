{
  "name": "MCS",
  "private": true,
  "dependencies": {
    "@fontsource/cabin": "^5.0.18",
    "@fontsource/lora": "^5.0.17",
    "@fortawesome/fontawesome-free": "^6.5.2",
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo-rails": "^8.0.5",
    "@popperjs/core": "^2.11.8",
    "@rails/actioncable": "7.1.3-4",
    "@rails/activestorage": "7.1.3-4",
    "@rails/ujs": "7.1.3-4",
    "autoprefixer": "^10.4.19",
    "axios": "^1.7.2",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "channels": "^0.0.4",
    "dropzone": "^6.0.0-beta.2",
    "esbuild": "^0.23.0",
    "html5sortable": "^0.14.0",
    "jquery": "^3.7.1",
    "lg-fullscreen": "^1.2.1",
    "lg-hash": "^1.0.4",
    "lg-pager": "^1.0.2",
    "lightgallery": "^2.7.2",
    "nodemon": "^3.1.3",
    "popper.js": "^1.16.1",
    "postcss": "^8.4.40",
    "postcss-cli": "^11.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.0",
    "react-images": "^1.2.0-beta.7",
    "react_ujs": "^3.2.1",
    "sass": "^1.77.5",
    "sortablejs": "^1.15.2",
    "stimulus": "^3.2.2",
    "swiper": "^11.1.12",
    "toastr": "^2.1.4",
    "turbolinks": "^5.2.0",
    "typeface-lora": "^1.1.13"
  },
  "version": "0.1.0",
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --format=esm --outdir=app/assets/builds --public-path=/assets",
    "build:css:compile": "sass --style compressed ./app/assets/stylesheets/application.scss:./app/assets/builds/application.css --no-source-map --load-path=node_modules",
    "build:css_admin:compile": "sass --style compressed ./app/assets/stylesheets/admin.scss:./app/assets/builds/admin.css --no-source-map --load-path=node_modules",
    "build:css:prefix": "postcss ./app/assets/builds/application.css --use=autoprefixer --output=./app/assets/builds/application.css",
    "build:css_admin:prefix": "postcss ./app/assets/builds/admin.css --use=autoprefixer --output=./app/assets/builds/admin.css",
    "build:css": "yarn build:css:compile && yarn build:css:prefix && yarn build:css_admin:compile && yarn build:css_admin:prefix",
    "watch:css": "nodemon --watch ./app/assets/stylesheets/ --ext scss --exec \"yarn build:css\""
  },
  "packageManager": "yarn@4.3.0",
  "browserslist": [
    "defaults"
  ]
}

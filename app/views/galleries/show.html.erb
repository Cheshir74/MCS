<link href="/css/lightgallery.css" rel="stylesheet">

<script type="text/javascript">
    $(document).ready(function(){
        $('#lightgallery').lightGallery({
            selector: '.item'
        });
    });
</script>

  <link rel="stylesheet" href="/css/mobirise-icons/mobirise-icons.css">
  <link rel="stylesheet" href="/css/style_mob.css">


<section class="mbr-gallery mbr-slider-carousel cid-qx5qFCU1Ss" id="gallery3-5" data-rv-view="360">

  <div>
    <div>
      <!-- Filter -->
      <!-- Gallery -->

      <div class="mbr-gallery-row">
        <div class="mbr-gallery-layout-default">
          <div>
            <div id="lightgallery">

              <% if @images.exists? %>
                <% @images.each_slice(6) do |image| %>

                  <% image.each do |image| %>



                    <div class="mbr-gallery-item mbr-gallery-item--pNaN">

                      <div class="item" data-src="<%= image.image_url() %>">
                        <a href='<%= image.image_url() %>'>
                          <%= image_tag image.image_url(:thumb) %>
                        </a>
                        <span class="icon-focus"></span>
                      </div>
                    </div>

                    <% end %>
                  <% end %>
              <% else %>
                <p>There are no images</p>
              <% end %>


            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

</section>


<script src="/js/masonry.pkgd.min.js"></script>

<script src="/js/scripttheme.js"></script>
<script src="/js/player.min.js"></script>
<script src="/js/scriptgal.js"></script>



 <div class="container-fluid">

 </div>


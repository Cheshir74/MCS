nav.navbar.navbar-light.bg-light
  .container-fluid
  ul.navbar-nav.ml-md-auto
    li.nav-item
      a.nav-link.px-1.mx-1.py-3.my-n2#sidebarCollapse href=""
        i.fas.fa-bars

    //far.fa-times-circle
